# Create a JSON file with API keys use this .env file to input the api keys via the API_KEYS_PATH
API_KEYS_PATH=./api_keys.json
# Optional: Inline configuration for AI Market News (default is GLM-4.6)
# API_KEYS='{"ai_news":{"provider":"glm","model":"glm-4.6","enable_network":true,"keys":["your_glm_key_primary","your_glm_key_backup"]}}'

# Switch the provider for AI Market News (defaults to glm, can be set to gemini)
# AI_NEWS_PROVIDER=glm
# Optional: Globally override the default model
# AI_NEWS_MODEL=glm-4.6

# GLM / Zhipu AI Credentials (used by default)
GLM_MODEL=glm-4.6
GLM_ENABLE_NETWORK=1
GLM_THINKING=enabled
GLM_KEY_1=your_glm_key_1
GLM_KEY_2=your_glm_key_2
GLM_KEY_3=your_glm_key_3

# Gemini Credentials (set AI_NEWS_PROVIDER=gemini to use)
GEMINI_MODEL=gemini-2.5-pro
GEMINI_ENABLE_NETWORK=1
GEMINI_KEY_1=your_gemini_key_1
GEMINI_KEY_2=your_gemini_key_2
GEMINI_KEY_3=your_gemini_key_3

# EDGAR user agent
EDGAR_USER_AGENT="DailyMessenger/1.0 (contact: [email protected])"

# Feishu webhooks
FEISHU_WEBHOOK_DAILY=your_feishu_daily_webhook
FEISHU_SECRET_DAILY=your_feishu_daily_secret
FEISHU_WEBHOOK_ALERTS=your_feishu_alerts_webhook
FEISHU_SECRET_ALERTS=your_feishu_alerts_secret

# OANDA for technical analysis
OANDA_TOKEN=your_oanda_practice_token

# Whether to allow Yahoo Finance as a last-resort fallback (0 = disabled, 1 = enabled)
# yfinance is default to no as the network environment complexity
YFINANCE_FALLBACK=0

# API keys defined below are for the fallback purpose, the project prefers to use the ones in the api_keys.json file

FINANCIAL_MODELING_PREP=your_fallback_financial_modeling_prep_api_key
TWELVE_DATA=your_fallback_twelve_data_api_key
FINNHUB=your_fallback_finnhub_api_key
TRADING_ECONOMICS_USER=your_fallback_trading_economics_user_api_key
TRADING_ECONOMICS_PASSWORD=your_fallback_trading_economics_password_api_key
ALPHA_VANTAGE=your_fallback_alpha_vantage_api_key
ALPACA_KEY_ID=your_fallback_alpaca_key_id
ALPACA_SECRET=your_fallback_alpaca_secret_api_key
